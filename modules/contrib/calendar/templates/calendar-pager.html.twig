{#
/**
 * @file
 * Default theme implementation for a calendar pager.
 *
 * Available variables:
 * - items: List of pager items.
 *
 * @ingroup themeable
 */
#}
{% if (items.previous or items.next) and not exclude %}
    <nav class="calendar-pager" role="navigation" aria-labelledby="pagination-heading">
        <h4 class="visually-hidden">{{ 'Pagination'|t }}</h4>
        <ul class="calendar-pager__items js-pager__items">
            {% if items.previous %}
                <li class="calendar-pager__item calendar-pager__item--control calendar-pager__item--previous">
                    <a href="{{ items.previous.url }}" title="{{ 'Go to previous page'|t }}" rel="prev"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
                        <span class="calendar-pager__item-title calendar-pager__item-title--backwards" aria-hidden="true">{{ items.previous.text|default('<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" aria-hidden="true" focusable="false">
    <path d="M15 19l-7-7 7-7" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
</svg>'|t) }}</span>
                        <span class="visually-hidden">{{ 'Previous'|t }}</span>
                    </a>
                </li>
            {% endif %}
            {% if items.current %}
                <li class="calendar-pager__item pager__item--current">{{ items.current }}</li>
            {% endif %}
            {% if items.next %}
                <li class="calendar-pager__item calendar-pager__item--control calendar-pager__item--next">
                    <a href="{{ items.next.url }}" title="{{ 'Go to next page'|t }}" rel="next"{{ items.next.attributes|without('href', 'title', 'rel') }}>
                        <span class="visually-hidden">{{ 'Next'|t }}</span>
                        <span class="calendar-pager__item-title calendar-pager__item-title--forward" aria-hidden="true">{{ items.next.text|default('<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" aria-hidden="true" focusable="false">
    <path d="M9 5l7 7-7 7" fill="none" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
</svg>'|t) }}</span>
                    </a>
                </li>
            {% endif %}
        </ul>
    </nav>
{% endif %}

