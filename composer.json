{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "1.x-dev",
        "drupal/block_class": "^1.3",
        "drupal/calendar": "^1.0@alpha",
        "drupal/captcha": "^1.2",
        "drupal/config_devel": "^1.8",
        "drupal/console": "^1.9",
        "drupal/core": "^9.3",
        "drupal/field_group": "^3.2",
        "drupal/file_entity": "^2.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/group": "^1.4",
        "drupal/image_field_caption": "^1.1",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/libraries": "^3.0@beta",
        "drupal/location_migration": "^1.0@beta",
        "drupal/migrate_file_to_media": "^2.0",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.1",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/mysql56": "^1.3",
        "drupal/og": "1.x-dev",
        "drupal/openid_connect": "^1.2",
        "drupal/override_node_options": "^2.6",
        "drupal/panels": "^4.6",
        "drupal/panels_ipe": "^4.6",
        "drupal/pathauto": "^1.8",
        "drupal/r4032login": "^2.1",
        "drupal/reroute_email": "^2.1",
        "drupal/scheduler": "^1.4",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_contextual_filters_or": "^1.3",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_migration": "^1.1",
        "drupal/viewsreference": "^1.7",
        "drush/drush": "^10.6",
        "oomphinc/composer-installers-extender": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "drupal/console": true,
            "composer/installers": true,
            "drupal/core-project-message": false,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "merge-plugin": {
	  "include": [
	     "modules/*/composer.json"
	  ],
	  "recurse": false,
	  "replace": false,
	  "merge-extra": false,
          "ignore-duplicates": true
        },
        "patches": {
          "drupal/core": {
            "https://www.drupal.org/node/2640994": "https://www.drupal.org/files/issues/2022-02-11/drupal-2640994-122-9.3.patch"
          },
          "drupal/openid_connect": {
            "https://www.drupal.org/node/3011413": "https://www.drupal.org/files/issues/2022-02-14/openid_connect-autologin-3011413-70.patch"
          },
          "drupal/og": {
            "https://github.com/zerolab/og/tree/196-redux": "patches/zerolab-og-196-redux-branch.patch"
          },
          "drupal/migrate_tools": {
            "https://www.drupal.org/node/2868762": "https://www.drupal.org/files/issues/2022-02-17/d7_migration_failing_on_d6_migrations-2868762-37.patch"
          }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
