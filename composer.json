{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "1.x-dev",
        "drupal/block_class": "^1.3",
        "drupal/calendar": "^1.0@alpha",
        "drupal/captcha": "^1.2",
        "drupal/config_devel": "^1.8",
        "drupal/core": "^9.3",
        "drupal/field_group": "^3.2",
        "drupal/file_entity": "^2.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/group": "^1.4",
        "drupal/image_field_caption": "^1.1",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/libraries": "^3.0@beta",
        "drupal/location_migration": "^1.0@beta",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.1",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/mysql56": "^1.3",
        "drupal/og": "1.0-alpha-7",
        "drupal/og_menu": "^1.0@alpha",
        "drupal/override_node_options": "^2.6",
        "drupal/panels": "^4.6",
        "drupal/panels_ipe": "^4.6",
        "drupal/pathauto": "^1.8",
        "drupal/reroute_email": "^2.1",
        "drupal/scheduler": "^1.4",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_migration": "^1.1",
        "drupal/viewsreference": "^1.7",
        "drupal/vppr": "^1.1",
        "drush/drush": "^10.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
          "drupal/migrate_tools": {
            "https://www.drupal.org/node/2868762": "https://www.drupal.org/files/issues/2022-02-17/d7_migration_failing_on_d6_migrations-2868762-37.patch"
          }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
